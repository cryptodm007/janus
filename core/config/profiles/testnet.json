{
  "profile": {
    "name": "testnet",
    "chain_ids": { "base": "base-sepolia", "solana": "devnet" }
  },
  "policy": {
    "min_sig_quorum": 0.67,
    "min_ai_score": 0.60,
    "max_clock_skew_sec": 60
  },
  "observability": {
    "log_level": "INFO",
    "metrics_enabled": true,
    "tracing_enabled": true
  },
  "bridge": {
    "poll_interval_sec": 10,
    "vendor": "base_solana",
    "network_timeout_sec": 10
  }
}
