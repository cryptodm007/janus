{
  "profile": {
    "name": "dev",
    "chain_ids": { "base": "base-local", "solana": "localnet" }
  },
  "policy": {
    "min_sig_quorum": 0.51,
    "min_ai_score": 0.50,
    "max_clock_skew_sec": 120
  },
  "observability": {
    "log_level": "DEBUG",
    "metrics_enabled": true,
    "tracing_enabled": true
  },
  "bridge": {
    "poll_interval_sec": 5,
    "vendor": "base_solana",
    "network_timeout_sec": 5
  }
}
